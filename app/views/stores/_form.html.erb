<%= simple_form_for(@store) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :name %>

    <%= f.input :player_limit %>

    <%= f.input :address %>

    <%= f.input :firm_id %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

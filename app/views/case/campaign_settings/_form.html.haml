= form_for @game_setting do |f|
  - if @game_setting.errors.any?
    #error_explanation
      %h2= "#{pluralize(@game_setting.errors.count, "error")} prohibited this game_setting from being saved:"
      %ul
        - @game_setting.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :wx_name
    = f.text_field :wx_name
  .field
    = f.label :game_id
    = f.number_field :game_id
  .field
    = f.label :wx_status
    = f.number_field :wx_status
  .actions
    = f.submit 'Save'

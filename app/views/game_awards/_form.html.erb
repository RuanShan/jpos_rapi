<%= simple_form_for(@game_award) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :game_round_id %>
    <%= f.input :position %>
    <%= f.input :name %>
    <%= f.input :prize_count %>
    <%= f.input :prize_name %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

<p id="notice"><%= notice %></p>

<h1>Game Round Assets</h1>

<table>
  <thead>
    <tr>
      <th>Game round</th>
      <th>Game player</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @game_round_assets.each do |game_round_asset| %>
      <tr>
        <td><%= game_round_asset.game_round_id %></td>
        <td><%= game_round_asset.game_player_id %></td>
        <td><%= link_to 'Show', game_round_asset %></td>
        <td><%= link_to 'Edit', edit_game_round_asset_path(game_round_asset) %></td>
        <td><%= link_to 'Destroy', game_round_asset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Game Round Asset', new_game_round_asset_path %>
